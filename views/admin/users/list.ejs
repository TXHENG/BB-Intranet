<%- include('../partials/header.ejs') -%>
<section>
    <h1 class="border-bottom">Member List</h1>
    <table class="table" id="member-list">
        <thead>
            <th>Rank</th>
            <th>Member name</th>
            <th>Email</th>
        </thead>
        <tbody>
            <% if(users.length > 0) { %>
                <% (users).forEach(user => { %>
                    <tr>
                        <td><%= user.rank %></td>
                        <td><%= user.name %></td>
                        <td><%= user.email %></td>
                    </tr>
                <% }) %>
            <% } %>
        </tbody>
    </table>
</section>
<script>
    $(document).ready(function(){
        
    });
</script>
